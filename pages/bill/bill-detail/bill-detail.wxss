.name {
  font-size: 2em;
  padding:30rpx;
  text-align: center;
}

.detail {
  display: flex;
  flex-direction: column;
}

navigator.newBill {
  position: fixed;
  margin: 0 auto;
  left: 0;
  right: 0;
  bottom: 30rpx;
  width: 150rpx;
  height: 150rpx;
  line-height: 140rpx;
  padding: 0;
  border: 1rpx solid rgb(192, 192, 192);
  border-radius: 30%;
  text-align: center;
  font-size: 1.5em;
}
